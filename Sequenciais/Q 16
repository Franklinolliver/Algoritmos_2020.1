#include <stdio.h>
#include <math.h>

int main(void) {
  int a, b, c;
  float d,x1,x2;

  printf("Valor do A: ");
  scanf("%d", &a);
  printf("Valor do B: ");
  scanf("%d", &b);
  printf("Valor do C: ");
  scanf("%d", &c);

  d = (pow(b,2)) - (4*a*c);
  x1 = (-b + sqrt(d)) / (2*a);
  x2 = (-b - sqrt(d)) / (2*a);

  printf("X1 = %.1F \nX2 = %.1f", x1,x2);
  
  return 0;
}
